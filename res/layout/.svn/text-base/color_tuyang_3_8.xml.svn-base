<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
 	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
   	android:layout_height="wrap_content"
	android:layout_centerInParent="true">
   	<ImageView 
    	android:id="@+id/ImageView01"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_alignParentLeft="true"
    	android:layout_alignParentTop="true"
    	android:layout_marginLeft="9dip"
    	android:layout_marginTop="9dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    <ImageView 
    	android:id="@+id/ImageView02"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_alignTop="@+id/ImageView01"
    	android:layout_toRightOf="@+id/ImageView01"
    	android:layout_marginLeft="11dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    <ImageView 
    	android:id="@+id/ImageView03"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_alignTop="@+id/ImageView01"
    	android:layout_toRightOf="@+id/ImageView02"
    	android:layout_marginLeft="11dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    	
    	
    <ImageView 
    	android:id="@+id/ImageView04"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/ImageView01"
    	android:layout_alignLeft="@+id/ImageView01"
    	android:layout_marginTop="10dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    <ImageView 
    	android:id="@+id/ImageView05"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/ImageView03"
    	android:layout_alignLeft="@+id/ImageView03"
    	android:layout_marginTop="10dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    	
    <ImageView 
    	android:id="@+id/ImageView06"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/ImageView04"
    	android:layout_alignLeft="@+id/ImageView04"
    	android:layout_marginTop="11dip"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    <ImageView 
    	android:id="@+id/ImageView07"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_toRightOf="@+id/ImageView06"
    	android:layout_alignLeft="@+id/ImageView02"
    	android:layout_alignTop="@+id/ImageView06"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    <ImageView 
    	android:id="@+id/ImageView08"   		
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/ImageView05"
    	android:layout_alignLeft="@+id/ImageView05"
    	android:layout_alignTop="@+id/ImageView06"
    	android:src="@drawable/cs_tuyang_item_3_0"/>
    	
    
    <ImageView 			
		android:layout_width="wrap_content"
    	android:layout_height="wrap_content"	
	   	android:layout_centerInParent="true"
   		android:src="@drawable/cs_tuyang_board_3_8"/>
</RelativeLayout>