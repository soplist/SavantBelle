package com.flextronics.cn.model;

/**用户选择的样本
 * 
 * @author ZhangGuoYin
 *
 */
public class ChoosedSample {
	
	/**
	 * 样本编号
	 */
	private int sample;
	/**
	 * 样本元素
	 */
	private String[] smapleElementChoosed;
	
	public int getSample() {
		return sample;
	}
	public void setSample(int sample) {
		this.sample = sample;
	}
	public String[] getSmapleElementChoosed() {
		return smapleElementChoosed;
	}
	public void setSmapleElementChoosed(String[] smapleElementChoosed) {
		this.smapleElementChoosed = smapleElementChoosed;
	}
	
	public ChoosedSample(int sample, String[] smapleElementChoosed) {
		super();
		this.sample = sample;
		this.smapleElementChoosed = smapleElementChoosed;
	}
	public ChoosedSample() {
		super();
	}	
}
