package com.flextronics.cn.model.hearing.memory;

import com.flextronics.cn.model.BaseParameter;
import com.flextronics.cn.model.ChoosedSample;

public class MemoryParameter extends BaseParameter {
	private String choosedMode;
	private String smapleType; 
	private String elementType;
	private String bitType; 
	private ChoosedSample musicalInstruments;
	private ChoosedSample scale;
	private boolean randomMusicalInstruments;
	private boolean randomScale;
	private int startBit;
	private int questionCount;
	public String getChoosedMode() {
		return choosedMode;
	}
	public void setChoosedMode(String choosedMode) {
		this.choosedMode = choosedMode;
	}
	public String getSmapleType() {
		return smapleType;
	}
	public void setSmapleType(String smapleType) {
		this.smapleType = smapleType;
	}
	public String getElementType() {
		return elementType;
	}
	public void setElementType(String elementType) {
		this.elementType = elementType;
	}
	public String getBitType() {
		return bitType;
	}
	public void setBitType(String bitType) {
		this.bitType = bitType;
	}
	public ChoosedSample getMusicalInstruments() {
		return musicalInstruments;
	}
	public void setMusicalInstruments(ChoosedSample musicalInstruments) {
		this.musicalInstruments = musicalInstruments;
	}
	public ChoosedSample getScale() {
		return scale;
	}
	public void setScale(ChoosedSample scale) {
		this.scale = scale;
	}
	public boolean isRandomMusicalInstruments() {
		return randomMusicalInstruments;
	}
	public void setRandomMusicalInstruments(boolean randomMusicalInstruments) {
		this.randomMusicalInstruments = randomMusicalInstruments;
	}
	public boolean isRandomScale() {
		return randomScale;
	}
	public void setRandomScale(boolean randomScale) {
		this.randomScale = randomScale;
	}
	public int getStartBit() {
		return startBit;
	}
	public void setStartBit(int startBit) {
		this.startBit = startBit;
	}
	public int getQuestionCount() {
		return questionCount;
	}
	public void setQuestionCount(int questionCount) {
		this.questionCount = questionCount;
	}
	
}