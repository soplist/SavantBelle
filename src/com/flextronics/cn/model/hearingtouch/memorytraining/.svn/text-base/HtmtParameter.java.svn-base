package com.flextronics.cn.model.hearingtouch.memorytraining;


import com.flextronics.cn.model.ChoosedSample;
import com.flextronics.cn.model.BaseParameter;

/**
 * <b>听觉触觉记忆训练</b></br>
 * 参数
 * @author ZhangGuoYin
 *
 */
public class HtmtParameter extends BaseParameter {

	/**
	 * 测试类型: 乐器或者音阶
	 */
	private int testType;
	/**
	 * 音乐类型：西洋乐，民族乐，打击乐
	 */
	private int musicType;
	/**
	 * 受测样本位元数
	 */
	private int bit;
	/**
	 * 已选音乐样本
	 */
	private ChoosedSample music;
	/**
	 * 优势手类型
	 */
	private int handType;
	/**
	 * 已选音阶样本
	 */
	private ChoosedSample scale;
	/**
	 * 题目数量
	 */
	private int questionCount;
	
	public int getTestType() {
		return testType;
	}
	public void setTestType(int testType) {
		this.testType = testType;
	}
	public int getMusicType() {
		return musicType;
	}
	public void setMusicType(int musicType) {
		this.musicType = musicType;
	}
	public int getBit() {
		return bit;
	}
	public void setBit(int bit) {
		this.bit = bit;
	}
	public ChoosedSample getMusic() {
		return music;
	}
	public void setMusic(ChoosedSample music) {
		this.music = music;
	}
	public int getHandType() {
		return handType;
	}
	public void setHandType(int handType) {
		this.handType = handType;
	}
	public ChoosedSample getScale() {
		return scale;
	}
	public void setScale(ChoosedSample scale) {
		this.scale = scale;
	}
	public int getQuestionCount() {
		return questionCount;
	}
	public void setQuestionCount(int questionCount) {
		this.questionCount = questionCount;
	}
}
